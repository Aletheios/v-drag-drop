{
    "rules": {
        "comma-dangle": [2,"never"],
        "no-cond-assign": [2,"always"],
        "no-console": 1,
        "no-constant-condition": 2,
        "no-control-regex": 1,
        "no-debugger": 1,
        "no-dupe-args": 2,
        "no-dupe-keys": 2,
        "no-duplicate-case": 2,
        "no-empty-character-class": 1,
        "no-empty": 2,
        "no-ex-assign": 2,
        "no-extra-boolean-cast": 2,
        "no-extra-parens": [1,"functions"],
        "no-extra-semi": 2,
        "no-func-assign": 2,
        "no-inner-declarations": [2,"both"],
        "no-invalid-regexp": 2,
        "no-irregular-whitespace": 2,
        "no-negated-in-lhs": 2,
        "no-obj-calls": 2,
        "no-regex-spaces": 2,
        "no-sparse-arrays": 1,
        "no-unreachable": 2,
        "use-isnan": 2,
        "valid-typeof": 2,
        "no-unexpected-multiline": 1,
        "accessor-pairs": [1,{"getWithoutSet":false,"setWithoutGet":true}],
        "block-scoped-var": 1,
        "consistent-return": 1,
        "curly": [2,"all"],
        "dot-notation": [2,{"allowKeywords":true,"allowPattern":"^[a-z]+(_[a-z]+)+$"}],
        "dot-location": [2,"property"],
        "eqeqeq": 2,
        "guard-for-in": 2,
        "no-alert": 2,
        "no-caller": 1,
        "no-const-assign": 2,
        "no-div-regex": 1,
        "no-else-return": 2,
        "no-eq-null": 2,
        "no-eval": 2,
        "no-extend-native": 2,
        "no-extra-bind": 1,
        "no-floating-decimal": 2,
        "no-implied-eval": 2,
        "no-iterator": 2,
        "no-labels": 1,
        "no-lone-blocks": 2,
        "no-loop-func": 1,
        "no-multi-spaces": 2,
        "no-native-reassign": 2,
        "no-new-func": 1,
        "no-new-wrappers": 1,
        "no-new": 1,
        "no-octal-escape": 2,
        "no-octal": 1,
        "no-proto": 2,
        "no-redeclare": [2,{"builtinGlobals":true}],
        "no-return-assign": [2,"except-parens"],
        "no-script-url": 2,
        "no-self-compare": 2,
        "no-sequences": 2,
        "no-unused-expressions": 2,
        "no-useless-call": 2,
        "no-void": 2,
        "no-warning-comments": [1,{"terms":["TODO","FIXME"],"location":"anywhere"}],
        "no-with": 2,
        "radix": 2,
        "wrap-iife": [2,"any"],
        "yoda": [2,"never"],
        "strict": [2,"global"],
        "no-catch-shadow": 2,
        "no-delete-var": 2,
        "no-label-var": 2,
        "no-shadow-restricted-names": 2,
        "no-shadow": [2,{"builtinGlobals":true,"hoist":"all","allow":["self","name","event"]}],
        "no-undef-init": 2,
        "no-undef": 2,
        "no-unused-vars": 2,
        "no-use-before-define": 2,
        "no-new-require": 1,
        "no-path-concat": 1,
        "brace-style": [2,"stroustrup",{}],
        "camelcase": 1,
        "comma-spacing": [2,{"after":true}],
        "comma-style": [2,"last"],
        "computed-property-spacing": [2,"never"],
        "consistent-this": [2,"self"],
        "indent": [2,4,{"VariableDeclarator":1,"SwitchCase":1}],
        "key-spacing": [2,{"afterColon":true}],
        "lines-around-comment": [2,{"beforeBlockComment":true}],
        "new-cap": [2,{"newIsCap":true,"capIsNew":true}],
        "new-parens": 2,
        "no-array-constructor": 2,
        "no-mixed-spaces-and-tabs": 2,
        "no-nested-ternary": 2,
        "no-new-object": 2,
        "no-spaced-func": 2,
        "no-trailing-spaces": [2,{"skipBlankLines":true}],
        "object-curly-spacing": [2,"always",{}],
        "padded-blocks": [2,{"switches":"never","classes":"never"}],
        "quote-props": [2,"as-needed"],
        "quotes": [2,"single",{"avoidEscape":false,"allowTemplateLiterals":true}],
        "semi-spacing": [2,{"after":true}],
        "semi": [2,"always"],
        "space-before-function-paren": [2,"never"],
        "space-in-parens": [2,"never"],
        "space-infix-ops": 2,
        "space-unary-ops": [2,{"words":true,"nonwords":false}],
        "spaced-comment": [2,"always",{}],
        "arrow-spacing": [2,{"before":true,"after":true}],
        "constructor-super": 2,
        "generator-star-spacing": [2,{"before":false,"after":true}],
        "no-var": 2,
        "prefer-spread": 1,
        "prefer-const": 1,
        "require-yield": 2
    },
    
    "parserOptions": {
        "parser": "babel-eslint",
        "sourceType": "module",
        "ecmaVersion": 2018
    },
    
    "env": {
        "browser": true,
        "es6": true
    },
    
    "root": true
}